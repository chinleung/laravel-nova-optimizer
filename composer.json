{
    "name": "chinleung/laravel-nova-optimizer",
    "description": "A package to optimize performance of Laravel Nova classes.",
    "keywords": [
        "chinleung",
        "laravel",
        "laravel-nova",
        "laravel-nova-optimizer"
    ],
    "homepage": "https://github.com/chinleung/laravel-nova-optimizer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chin Leung",
            "email": "hello@chinleung.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0|^7.0",
        "laravel/nova": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ChinLeung\\LaravelNovaOptimizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChinLeung\\LaravelNovaOptimizer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
